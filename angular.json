{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "leiding-beheer": {
      "root": "src",
      "sourceRoot": "frontend",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "wwwroot",
            "index": "frontend/index.html",
            "main": "frontend/main.ts",
            "tsConfig": "frontend/tsconfig.app.json",
            "polyfills": "frontend/polyfills.ts",
            "assets": [
              "./frontend/assets",
              "./frontend/favicon.ico"
            ],
            "styles": [
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/simple-line-icons/css/simple-line-icons.css",
              "frontend/scss/style.scss",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/ng-snotify/styles/material.css",
              "node_modules/@ng-select/ng-select/themes/material.theme.css"
            ],
            "scripts": [
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/chart.js/dist/Chart.min.js",
              "node_modules/auth0-js/build/auth0.js"
            ]
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "frontend/environments/environment.ts",
                  "with": "frontend/environments/environment.dev.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "frontend/environments/environment.ts",
                  "with": "frontend/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "leiding-beheer:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "leiding-beheer:build:dev"
            },
            "production": {
              "browserTarget": "leiding-beheer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "leiding-beheer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "frontend/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "frontend/polyfills.ts",
            "tsConfig": "frontend/tsconfig.spec.json",
            "scripts": [
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/chart.js/dist/Chart.min.js",
              "node_modules/auth0-js/build/auth0.js"
            ],
            "styles": [
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/simple-line-icons/css/simple-line-icons.css",
              "frontend/scss/style.scss",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/ng-snotify/styles/material.css",
              "node_modules/@ng-select/ng-select/themes/material.theme.css"
            ],
            "assets": [
              "frontend/assets",
              "frontend/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "leiding-beheer-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "leiding-beheer:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "leiding-beheer",
  "schematics": {
    "@schematics/angular:component": {
      "spec": false,
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:service": {
      "spec": false
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}