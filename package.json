{
  "name": "leiding-beheer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./set-env.ts",
    "totems": "ng serve kangoeroes-frontend-totems --proxy-config proxy.config.json --port=4300 --configuration=dev",
    "leidingbeheer": "ng serve kangoeroes-frontend-leidingbeheer --configuration=dev --proxy-config proxy.config.json --port=4200",
    "build": "ng build --configuration=production",
    "build-totems-prod": "npm run git-version && ng build kangoeroes-frontend-totems --configuration=production --outputPath=dist/totems",
    "build-totems-staging": "npm run git-version && ng build kangoeroes-frontend-totems --configuration=staging --outputPath=dist/totems-staging",
    "build-leidingbeheer-staging": "npm run git-version && ng build kangoeroes-frontend-leidingbeheer --configuration=staging --outputPath=dist/leidingbeheer-staging",
    "build-leidingbeheer-production": "npm run git-version && ng build kangoeroes-frontend-leidingbeheer --configuration=production --outputPath=dist/leidingbeheer-production",
    "test": "ng test",
    "lint": "ng lint",
    "lint-core": "ng lint kangoeroes-frontend-core",
    "lint-totems": "ng lint kangoeroes-frontend-totems",
    "lint-leidingbeheer": "ng lint kangoeroes-frontend-core",
    "git-version": "node scripts/git-version-gen.js",
    "e2e": "ng e2e"
  },
  "post-commit": [
    "git-version"
  ],
